{
  "name": "generator-mcfly-ng2",
  "version": "0.0.19",
  "description": "A yeoman generator for scaffolding an app using angular2 and webpack",
  "homepage": "https://github.com/mcfly-io/generator-mcfly-ng2",
  "repository": {
    "type": "git",
    "url": "git://github.com/mcfly-io/generator-mcfly-ng2"
  },
  "scripts": {
    "eslint": "eslint .",
    "lint": "npm run eslint",
    "mocha": "istanbul cover --root ./generators  --include-all-sources -x **/templates/** --dir ./coverage/mocha --report text --report text-summary --report lcov --print none  _mocha -- test/mocha/**/*.spec.js --reporter spec --timeout 10000",
    "mocha.watch": "mocha test/mocha/**/*.spec.js -R nyan -w --timeout 10000",
    "pretest": "npm run eslint",
    "test": "npm run mocha",
    "release.full": "gulp release:full"
  },
  "author": {
    "name": "Avi Haiat",
    "email": "thaiat@ipelia.fr",
    "url": "https://github.com/thaiat"
  },
  "files": [
    "generators",
    "libs",
    ".jsbeautifyrc"
  ],
  "main": "generators/app/index.js",
  "preferGlobal": true,
  "keywords": [
    "yeoman-generator",
    "ng2",
    "webpack",
    "angular2",
    "generator",
    "yeoman",
    "starter",
    "boilerplate",
    "scaffold"
  ],
  "license": "MIT",
  "dependencies": {
    "chalk": "^1.1.1",
    "gulp-filter": "^3.0.1",
    "gulp-tap": "0.1.3",
    "js-beautify": "beautify-web/js-beautify",
    "lodash": "3.10.1",
    "mkdirp": "^0.5.1",
    "strip-json-comments": "^2.0.0",
    "underscore.string": "^3.2.3",
    "update-notifier": "^0.6.0",
    "yeoman-generator": "^0.22.4",
    "yosay": "^1.1.0"
  },
  "devDependencies": {
    "bluebird": "^3.1.4",
    "chai": "^3.5.0",
    "chalk": "1.1.1",
    "coveralls": "^2.11.6",
    "del": "^2.2.0",
    "eslint": "^1.10.3",
    "eslint-plugin-nodeca": "^1.0.3",
    "event-stream": "^3.3.2",
    "github": "0.2.4",
    "github-username": "2.1.0",
    "gulp": "3.9.0",
    "gulp-bump": "1.0.0",
    "gulp-concat": "2.6.0",
    "gulp-exec": "2.1.2",
    "gulp-git": "^1.7.0",
    "gulp-help": "1.6.1",
    "gulp-if": "2.0.0",
    "gulp-mux": "0.2.24",
    "gulp-order": "1.1.1",
    "gulp-util": "3.0.7",
    "istanbul": "^0.4.2",
    "mocha": "^2.4.5",
    "require-dir": "0.3.0",
    "run-sequence": "^1.1.5",
    "strip-json-comments": "2.0.0",
    "yargs": "^3.32.0",
    "yeoman-assert": "^2.1.1",
    "yeoman-test": "^1.0.0"
  }
}
